<%= form_for @event do |f| %>
  <%= f.label :title, 'Title' %>
  <%= f.text_field :title %>
  <%= f.label :description, 'Description' %>
  <%= f.text_field :description %>
  <%= f.label :from, 'From' %>
  <%= f.datetime_select :from %>
  <%= f.label :to, 'To' %>
  <%= f.datetime_select :to %>

  <%= f.submit %>
<% end %>
